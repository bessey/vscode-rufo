{
  "name": "vscode-rufo",
  "displayName": "rufo (Ruby formatter)",
  "description": "Ruby formatter",
  "version": "0.1.1",
  "publisher": "mbessey",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Formatters"
  ],
  "keywords": [
    "ruby",
    "rufo",
    "formatting"
  ],
  "activationEvents": [
    "onCommand:extension.formatRuby",
    "onLanguage:ruby",
    "onLanguage:gemfile",
    "onLanguage:erb"
  ],
  "main": "./out/extension.js",
  "homepage": "https://github.com/bessey/vscode-rufo",
  "bugs": {
    "url": "https://github.com/bessey/vscode-rufo/issues"
  },
  "repository": {
    "url": "https://github.com/bessey/vscode-rufo"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "galleryBanner": {
    "color": "#CC342D",
    "theme": "dark"
  },
  "contributes": {
    "configurationDefaults": {
      "[ruby]": {
        "editor.formatOnSave": true
      },
      "[gemfile]": {
        "editor.formatOnSave": true
      },
      "[erb]": {
        "editor.formatOnSave": true
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "typescript": "^3.6.4",
    "tslint": "^5.20.0",
    "vscode-test": "^1.2.2"
  },
  "__metadata": {
    "id": "71552028-35dc-4e20-b904-8fe8543944a1",
    "publisherDisplayName": "Matt Bessey",
    "publisherId": "05e66fec-b8cb-4253-8ed1-1e8cebaa67ef"
  }
}
